{
  "name": "otrie",
  "description": "Observable Path Trie Store",
  "version": "1.2.0",
  "license": "Apache-2.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "private": false,
  "scripts": {
    "prebuild": "npm run clean",
    "build": "webpack",
